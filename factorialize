function factorialize(num) {
  
  var numArr = [];                          //creates place to store our array of numbers between 1 and num
  for(var i = 1; i <= num; i++){            //creates an array of numbers between 1 and num
    numArr.push(i);
  }
  
  var arrProduct = 1;                       //creates place to store the product of all the numbers in our numArr
  for(var j = 0; j < numArr.length; j++){   //this multiplies all of the numbers in the array, and stores the product in our variable                                                   arrProduct variable
    arrProduct *= numArr[j];
  }
  
  console.log(arrProduct);
}

factorialize(11);
